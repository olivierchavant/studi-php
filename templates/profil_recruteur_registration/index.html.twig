{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
<div class="container"> 
    <h1>Votre profil ! </h1>

    {{ form_start(registrationForm) }}
        {{ form_row(registrationForm.Entreprise) }}
        {{ form_row(registrationForm.Fonction) }}
        {{ form_row(registrationForm.Adresse) }}
        {{ form_row(registrationForm.tel) }}

        <button type="submit" class="btn btn-lg btn-success">Enregistrer</button>
        <button class="btn btn-lg btn-success"  onclick="history.go(-1);"> retour  </button>
    {{ form_end(registrationForm) }}
</div>
{% endblock %}
