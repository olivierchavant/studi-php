{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
<div class="container"> 
    <h1>Votre profil ! </h1>

    {{ form_start(registrationForm) }}
        {{ form_row(registrationForm.titre) }}
        {{ form_row(registrationForm.poste) }}
        {{ form_row(registrationForm.domaine) }}
        {{ form_row(registrationForm.salaire) }}
        {{ form_row(registrationForm.description) }}

        <button type="submit" class="btn">Register</button>
    {{ form_end(registrationForm) }}
</div>
{% endblock %}
